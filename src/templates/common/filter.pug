              label
                input(type="radio" name="where" value="EURO/сотка")
                p EURO/сотка
              label
                input(type="radio" name="where" value="Воронеж")
                p Воронеж
      .inpBlock.inpBlock_190
        .inpBlock__title Коммуникации
        .inpBlock-box
          .inpBlock-select
            .inpBlock-select-view
