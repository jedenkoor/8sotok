section.filter
  form.container.filter-container
    .filter-main
      .inpBlock.inpBlock_160
        .inpBlock__title Тип недвижимости
        .inpBlock-box
          .inpBlock-select
            .inpBlock-select-view
              span Участок
              <svg width="8" height="6" viewBox="0 0 8 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M1 1L4 4L7 1" stroke="black" stroke-width="1.5"/>
              </svg>
            .inpBlock-select-list
              label
                input(type="checkbox" name="typeNedvizh" value="Участок" checked)
                p Участок
              label
                input(type="checkbox" name="typeNedvizh" value="Поселок")
                p Поселок
              label
                input(type="checkbox" name="typeNedvizh" value="Дома")
                p Дома
              label
                input(type="checkbox" name="typeNedvizh" value="Таунхаусы")
