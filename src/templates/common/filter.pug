      .inpBlock.inpBlock_200
        .inpBlock__title Расположение
        .inpBlock-box
          .inpBlock-select
            .inpBlock-select-view
              span Санкт-Петербург
              <svg width="8" height="6" viewBox="0 0 8 6" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M1 1L4 4L7 1" stroke="black" stroke-width="1.5"/>
              </svg>
            .inpBlock-select-list
              label
                input(type="radio" name="where" value="Санкт-Петербург" checked)
                p Санкт-Петербург
              label
                input(type="radio" name="where" value="Москва")
